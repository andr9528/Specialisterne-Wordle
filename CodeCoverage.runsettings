<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="Code Coverage">
        <Configuration>
          <CodeCoverage>
              <Functions>
                <Exclude>
                  <Function>^Wordle\.Persistence\.Migrations\..*</Function>
                  <Function>^Wordle\.Tests\.GlobalStaticResources\..*</Function>
                </Exclude>
              </Functions>
            </CodeCoverage>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>
